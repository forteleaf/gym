package com.gym.app.dto;

import java.sql.Date;

public class InstructorDto {
	private int inum;
	private String id;
	private Date ijoin;
	private int work;
	private int sal;
	private String info;
	public InstructorDto() {}
	public InstructorDto(int inum, String id, Date ijoin, int work, int sal, String info) {
		super();
		this.inum = inum;
		this.id = id;
		this.ijoin = ijoin;
		this.work = work;
		this.sal = sal;
		this.info = info;
	}
	public int getInum() {
		return inum;
	}
	public void setInum(int inum) {
		this.inum = inum;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public Date getIjoin() {
		return ijoin;
	}
	public void setIjoin(Date ijoin) {
		this.ijoin = ijoin;
	}
	public int getWork() {
		return work;
	}
	public void setWork(int work) {
		this.work = work;
	}
	public int getSal() {
		return sal;
	}
	public void setSal(int sal) {
		this.sal = sal;
	}
	public String getInfo() {
		return info;
	}
	public void setInfo(String info) {
		this.info = info;
	}
}

