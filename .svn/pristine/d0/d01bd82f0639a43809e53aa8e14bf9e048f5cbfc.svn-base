package com.gym.app.dto;

import java.sql.Date;

public class AndroidTrainDto {
	
	private String uname;
	private String phone;
	private String savefile;
	private String iid;
	private int attendance;
	
	
	public AndroidTrainDto(){}

	public AndroidTrainDto(String uname,String phone,String savefile,String iid,int attendance) {
		super();
		
		this.uname = uname;
		this.phone = phone;	
		this.savefile = savefile;	
		this.iid = iid;
		this.attendance = attendance;
		
	}

	public String getUname() {
		return uname;
	}

	public void setUname(String uname) {
		this.uname = uname;
	}
	public String getPhone() {
		return phone;
	}

	public void setAddr(String phone) {
		this.phone = phone;
	}

	
	public String getSavefile() {
		return savefile;
	}

	public void setSavefilename(String savefile) {
		this.savefile = savefile;
	}

	
	public String getIid() {
		return iid;
	}

	public void setIid(String iid) {
		this.iid = iid;
	}


	public int getAttendance() {
		return attendance;
	}

	public void setAttendance(int attendance) {
		this.attendance = attendance;
	}

}
